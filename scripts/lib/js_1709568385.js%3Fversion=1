// FROM: scripts/jquery-custom.js

function getElementOffsetToCenterOfScreen(el) {
    var elOffset = el.offset().top;
    var elHeight = el.height();
    var windowHeight = $(window).height();
    var offset;
    if (elHeight < windowHeight) {
        offset = elOffset - (windowHeight / 2 - elHeight / 2);
    } else {
        offset = elOffset;
    }
    return offset;
}
function addLoaderProducts() {
    if ($(".products_container").length) {
        $(".products_container").prepend(
            '<div class="customloaderProductsContainer"><div class="customloaderProducts"></div></div>'
        );
    }
}

function removeLoaderProducts() {
    if ($(".products_container").length) {
        var loader = $(".products_container").find(
            ".customloaderProductsContainer"
        );
        if (loader.length) {
            loader.remove();
        }
    }
}
function addLoader(el) {
    if (el.length) {
        el.prepend(
            '<div class="customloaderContainer"><div class="customloader"></div></div>'
        );
    }
}

function removeLoader(el) {
    if (el.length) {
        var loader = el.find(".customloaderContainer");
        if (loader.length) {
            loader.remove();
        }
    }
}
function replaceHtml(el, html) {
    var oldEl = typeof el === "string" ? document.getElementById(el) : el;
    var newEl = oldEl.cloneNode(false);
    newEl.innerHTML = html;
    oldEl.parentNode.replaceChild(newEl, oldEl);
    return newEl;
}

/*scroll lazy function */
$.fn.isInViewport = function () {
    var elementTop = $(this).offset().top;
    var elementBottom = elementTop + $(this).outerHeight();
    var viewportTop = $(window).scrollTop();
    var viewportBottom = viewportTop + $(window).height();
    return (parseFloat(elementBottom) > (parseFloat(viewportTop) - 1000)) && (parseFloat(elementTop) < (parseFloat(viewportBottom) + 1000));
};
function initializeSlick() {
    /*$('.products-carousel').slick({
            dots: true,
            arrows:false,
            infinite: false,
            speed: 500,
            slidesToShow: 5,
            slidesToScroll: 3,
            autoplay: false,
            autoplaySpeed: 6000,
            responsive: [
            {
                breakpoint: 1600,
                settings: {

                    slidesToShow: 3,
                    slidesToScroll: 3
                }
            },
            {
                breakpoint: 991,
                settings: {

                    slidesToShow: 3,
                    slidesToScroll: 3,
                    autoplay: false
                }
            },
            {
                breakpoint: 767,
                settings: {

                    slidesToShow: 2,
                    slidesToScroll: 2,
                    autoplay: false
                }
            }
            ]  
        });*/
    $(".products-carousel:not(.jsHomeAjaxCarousel)").slick({
        dots: true,
        arrows: true,
        infinite: true,
        speed: 500,
        slidesToShow: 2,
        slidesToScroll: 2,
        autoplay: false,
        responsive: [
            {
                breakpoint: 1600,
                settings: {
                    slidesToShow: 3,
                    slidesToScroll: 3,
                },
            },
            {
                breakpoint: 991,
                settings: {
                    slidesToShow: 3,
                    slidesToScroll: 3,
                    autoplay: false,
                },
            },
            {
                breakpoint: 767,
                settings: {
                    slidesToShow: 2,
                    slidesToScroll: 2,
                    autoplay: false,
                },
            },
        ],
    });

    $(".products-carousel.jsHomeAjaxCarousel").slick({
        dots: true,
        arrows: true,
        infinite: true,
        speed: 500,
        slidesToShow: 1,
        slidesToScroll: 1,
        autoplay: false,
        responsive: [
            {
                breakpoint: 1600,
                settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1,
                },
            },
        ],
    });
}

function destroyCarousel() {
    if ($(".products-carousel").hasClass("slick-initialized")) {
        $(".products-carousel").slick("destroy");
    }
}

var attributes_combinations = [];
var checker = (arr, target) => target.every((v) => arr.includes(v));
function initializeAttributesCombinations() {
    if ($("#attributes_combinations").length) {
        var temp = JSON.parse($("#attributes_combinations").html());
        for (var prop in temp) {
            attributes_combinations.push(temp[prop]);
        }
    }
}
function attributesCombinationsManager(
    attributes_combinations,
    selected_attributes
) {
    if (attributes_combinations.length === 0) {
        initializeAttributesCombinations();
    }
    for (var prop in attributes_combinations) {
        found = checker(attributes_combinations[prop], selected_attributes);
        if (found) {
            for (var innerprop in attributes_combinations[prop]) {
                $(
                    ".attribute-" + attributes_combinations[prop][innerprop]
                ).removeAttr("disabled");
                $(".attribute-" + attributes_combinations[prop][innerprop])
                    .closest(".radio-holder")
                    .removeClass("inactive");
            }
        }
    }
    validateAddToCart();
}
function validateAddToCart() {
    var maxAttributesCategories = $(".options .pfield").length;
    var attributesCategoriesChecked = 0;
    $(".options .pfield").each(function () {
        if ($(this).find('input[type="radio"]:checked').length > 0) {
            attributesCategoriesChecked++;
        }
    });
    if (attributesCategoriesChecked < maxAttributesCategories) {
        $(".shop-btn").attr("disabled", "disabled");
        $(".cart_quantity_form").addClass("enable-add-to-cart");
        return false;
    } else {
        $(".shop-btn").removeAttr("disabled");
        $(".cart_quantity_form").addClass("enable-add-to-cart");
        return true;
    }
}
function getSelectedAttributes() {
    /*Get selected attributes ids sorted in ascending order*/
    var attributes = [];
    $(".attributes-combination-action:checked").each(function () {
        attributes.push(parseInt($(this).val()));
    });
    attributes.sort(function (a, b) {
        return a - b;
    });
    return attributes;
}

function cartesian(...args) {
    var r = [],
        max = args.length - 1;
    function helper(arr, i) {
        for (var j = 0, l = args[i].length; j < l; j++) {
            var a = arr.slice(0); /* clone arr*/
            a.push(args[i][j]);
            if (i == max) r.push(a);
            else helper(a, i + 1);
        }
    }
    helper([], 0);
    return r;
}

function getValidAttributesCombinations() {
    /*Get selected attributes ids sorted in ascending order*/
    var attributes = [];
    var attributes_2 = [];
    var attributes_3 = [];
    var attributeCounter = 1;
    $(".pfield").each(function () {
        var pfield = $(this);
        pfield.find(".attributes-combination-action").each(function () {
            if ($(this).attr("disabled") === undefined) {
                if (attributeCounter === 1) {
                    attributes.push(parseInt($(this).val()));
                } else if (attributeCounter === 2) {
                    attributes_2.push(parseInt($(this).val()));
                } else if (attributeCounter === 3) {
                    attributes_3.push(parseInt($(this).val()));
                }
            }
        });
        attributeCounter++;
    });
    var allCombs;
    if (attributeCounter === 1) {
        attributes.sort(function (a, b) {
            return a - b;
        });
        allCombs = cartesian(attributes);
    } else if (attributeCounter === 2) {
        attributes.sort(function (a, b) {
            return a - b;
        });
        attributes_2.sort(function (a, b) {
            return a - b;
        });
        allCombs = cartesian(attributes, attributes_2);
    } else {
        attributes.sort(function (a, b) {
            return a - b;
        });
        attributes_2.sort(function (a, b) {
            return a - b;
        });
        attributes_3.sort(function (a, b) {
            return a - b;
        });
        allCombs = cartesian(attributes, attributes_2, attributes_3);
    }
    return allCombs;
}

function updateAttributePrice() {
    var firstChecked = $('.pfield[data-sort="0"]').find(
        ".attributes-combination-action:checked"
    );
    var product_id = $('input[type="hidden"][name="products_id"]').val();
    $.ajax({
        type: "POST",
        url: "../update_attribute_price.php",
        data: {
            products_id: product_id,
            options_id: firstChecked.attr("data-attribute-category"),
            options_values_id: firstChecked.val(),
            customer_group_id: $(
                'input[name="customer_group_id"][type="hidden"]'
            ).val(),
        },
        success: function (response) {
            if (response && response.length > 0) {
                var obj = JSON.parse(response);
                if ($(".productprice.wholesale").length) {
                    if (obj["originalPriceClean"] !== undefined) {
                        $(
                            ".finalPriceClean:not(.productSpecialPrice) .previousPrice"
                        ).html(obj["originalPriceClean"]);
                    }
                    if (obj["originalPrice"] !== undefined) {
                        $(
                            ".productSpecialPrice.finalPrice .previousPrice"
                        ).html(obj["originalPrice"]);
                    }
                    if (obj["priceClean"] !== undefined) {
                        if (
                            $(
                                ".finalPriceClean .productSpecialPrice.finalPrice"
                            ).length
                        ) {
                            $(
                                ".finalPriceClean .productSpecialPrice.finalPrice"
                            ).html(obj["priceClean"]);
                        } else if ($(".price.finalPriceClean").length) {
                            $(".price.finalPriceClean").html(obj["priceClean"]);
                        }
                    }
                    if (obj["price"] !== undefined) {
                        if (
                            $(
                                ".productSpecialPrice.finalPrice .productSpecialPrice.finalPrice"
                            ).length
                        ) {
                            $(
                                ".productSpecialPrice.finalPrice .productSpecialPrice.finalPrice"
                            ).html(obj["price"]);
                        } else if (
                            $(".productSpecialPrice.finalPrice").length
                        ) {
                            $(".productSpecialPrice.finalPrice").html(
                                obj["price"]
                            );
                        }
                    }
                } else {
                    if (obj["originalPrice"] !== undefined) {
                        if ($(".productprice .previousPrice").length) {
                            $(".productprice .previousPrice").html(
                                obj["originalPrice"]
                            );
                        }
                    }
                    if (obj["price"] !== undefined) {
                        if (
                            $(".productprice .productSpecialPrice.finalPrice")
                                .length
                        ) {
                            $(
                                ".productprice .productSpecialPrice.finalPrice"
                            ).html(obj["price"]);
                        } else if ($(".productprice .finalPrice").length) {
                            $(".productprice .finalPrice").html(obj["price"]);
                        }
                    }
                }
            }
        },
    });
}

function updateProductImage(radiobtn) {
    var product_id = radiobtn.attr("data-product-id");
    var option_id = radiobtn.attr("data-attribute-category");
    var option_value_id = radiobtn.val();

    var image_div = $(document).find(
        ".productinfo .productphotogallery .image"
    );
    $(image_div).css("min-height", $(image_div).height());
    $.ajax({
        type: "POST",
        url: "../update_attribute_image.php",
        data: {
            products_id: product_id,
            options_id: option_id,
            options_values_id: option_value_id,
        },
        success: function (response) {
            var obj = JSON.parse(response);
            $("#main_product_image_link").attr("href", obj["href"]);
            $("#main_product_image_link img").attr("src", obj["src"]);
        },
    });

    cc = -1;
}
function triggerAttributesCombinations() {
    var el = $(".attributes-combination-action:checked").last();
    el.trigger("click");
    el.prop("checked", "true").trigger("change");
}
function initializeProductInfoAttributes() {
    var options = $(".pfield").length;

    var multiple_attributes = false;
    $(".pfield").each(function () {
        var values = $(this).find(".radio-holder").length;
        if (values > 1) {
            multiple_attributes = true;
            return false;
        }
    });
    if (!multiple_attributes) {
        $(".attributes-combination-action").each(function () {
            $(this).prop("checked", "true");
            $(this).trigger("change");
        });
    }
    handleClearSelections();
    updateAttributePrice();
}
function clearSelections() {
    $(".attributes-combination-action:checked").each(function () {
        $(this).prop("checked", "false");
        $(this).trigger("click");
        $(this).trigger("change");
    });
}
function handleClearSelections() {
    if ($(".attributes-combination-action:checked").length > 0) {
        $(".clear-selections").show();
    } else {
        $(".clear-selections").hide();
    }
}
function getProductMaxQuantity() {
    var selections = getSelectedAttributes();
    var max_quantity = 99;
    var attributes_max_quantity_set = false;
    if (
        selections.length > 0 &&
        $("#attributes_combinations_quantities").length
    ) {
        var selectionsKey = selections.join();
        var obj = JSON.parse($("#attributes_combinations_quantities").html());
        if (obj[selectionsKey] !== undefined) {
            attributes_max_quantity_set = true;
            max_quantity = parseInt(obj[selectionsKey]);
        }
    }
    if (
        $('input[type="hidden"][name="main_max_quantity"]').length > 0 &&
        !attributes_max_quantity_set
    ) {
        max_quantity = parseInt(
            $('input[type="hidden"][name="main_max_quantity"]').val()
        );
    }
    return max_quantity;
}

function createIframe(siteVideoId) {
    var iframe = document.createElement("iframe");
    iframe.setAttribute(
        "src",
        "//www.youtube.com/embed/" +
        siteVideoId +
        "?enablejsapi=1&version=3&playerapiid=ytplayer&autoplay=1&rel=0"
    );
    iframe.setAttribute("frameborder", "0");
    iframe.setAttribute("allowfullscreen", "1");
    iframe.setAttribute("id", siteVideoId + "-video");
    return iframe;
}

function newsletterCaptchaReveal() {
    var captcha = $(".jsNewsletterCaptcha");
    var input = $(".jsNewletterInput");
    var submit = $(".jsNewsletterSubmit");
    input.click(function () {
        captcha.fadeIn();
    });

    submit.click(function () {
        captcha.fadeIn();
    });
}

