    /* newsletter form validation */
    function newsletterValidateEmail(mail) {
        if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(mail)) {
            return true;
        }
        return false;
    }
    $("#SubscribeForm").submit(function () {
        var email_to_validate = $("#SubscribeForm #email").val();
        var is_valid = newsletterValidateEmail(email_to_validate);
        if (!is_valid) {
            $("#invalid_newsletter_email").show();
            $("#SubscribeForm #email").addClass("error");
        }
        return is_valid;
    });

    /* filters items conditions */
    if ($(".filters-wrap").find(".extended_filters_category").length > 4) {
        $("#filtersModal").addClass("expanded-modal");
    }
    if ($(".filters-wrap").find(".extended_filters_category").length < 3) {
        $(".filters-wrap").addClass("small");
    }

    /*if ($(".product-gallery .extra-images").length) {
        $(".product-gallery .extra-images").slick({
