    $(".specials_current_category_id").change(function () {
        $(this).closest("form").submit();
    });
    /*extreme lazy
    [].forEach.call(document.querySelectorAll('img[data-src]'), function(img) {
        img.setAttribute('src', img.getAttribute('data-src'));
        img.onload = function() {
            img.removeAttribute('data-src');
        };
    });   */
    $("a.hassubmenu").click(function () {
        if ($(this).parent().hasClass("active")) {
            $("li.hassubmenu").removeClass("active");
            $("ul.menu").slideUp();
        } else {
            $("li.hassubmenu").removeClass("active");
            $(this).parent().toggleClass("active");
            $("li:not(.active) ul.menu").slideUp();
            $(this).parent().find("ul.menu").slideToggle();
        }
    });

    $(document).on("click", ".quickview", function (e) {
        e.preventDefault();
        var id = $(this)
            .closest(".productbox, .buy-also-item, .product_set")
            .attr("data-id");
        var origin;

        if ($(this).hasClass("buyalsoaction")) {
            origin = "buyalso";
        } else {
            origin = "productlisting";
        }
        if (attributes_combinations.length > 0) {
            attributes_combinations = [];
        }
        /*.product_quickview_box*/
        var formData = {
            products_id: id,
            origin: origin /*, 'language': $('#language').val()*/,
        };
        $.ajax({
            url: "ajax_quickview.php",
            type: "get",
            data: formData,
            success: function (response) {
                replaceHtml(
                    document.getElementById("quickviewContent"),
                    response
                );

                setTimeout(function () {
                    $("#quickviewModal .images").slick({
                        dots: true,
                        arrows: false,
                        infinite: false,
                        speed: 500,
                        slidesToShow: 1,
                        slidesToScroll: 1,
                    });
                }, 500);

                $("#quickviewModal").modal("show");
                initializeProductInfoAttributes();
            },
            error: function (xhr, desc, err) {
                console.log(xhr);
                console.log("Details: " + desc + "\nError:" + err);
            },
        });
        return false;
    });
    $(document).on("click", ".i_close_quickview", function (e) {
        $(this).parent().parent().removeClass("active");
    });
    $(document).on(
        "change",
        ".product_quickview_box .attribute_select",
        function (e) {
            var prod_id = $(this)
                .closest(".product_quickview_box")
                .attr("data-id");
            var option_id = $(this).attr("name");
            var matches = option_id.match(/\[(.*?)\]/);
            if (matches) {
                option_id = matches[1];
            }
            var option_value_id = $(this).val();
            var selector_class =
                ".attribute_image_" + option_id + "_" + option_value_id;
        }
    );

    $(window).scroll(function () {
        if ($(window).scrollTop() > 0) {
        } else {
        }

        if ($(window).width() > 1024) {
            /*sticky add to cart */
            if ($(window).scrollTop() > 420) {
                if ($(".box_widget.top").length) {
                    $(".box_widget.top").show();
                    $(".box_widget.top").css("top", "70px");
                    $(".box_widget.top").css("bottom", "unset");
                }
            }
            if ($(window).scrollTop() < 420) {
                if ($(".box_widget.top").length) {
                    $(".box_widget.top").hide();
                    $(".box_widget.top").css("top", "-75px");
                }
            }
        } else {
            /*sticky add to cart mobile */
            if ($(window).scrollTop() > 1220) {
                if ($(".box_widget.top").length) {
                    $(".box_widget.top").show();

                    $(".to-top-icons").css("bottom", "60px");
                    $("#chat-application").addClass("bottom-add-cart");

                    $(".box_widget.top").css("bottom", "0px");
                    $(".box_widget.top").css("top", "unset");
                }
            }
            if ($(window).scrollTop() < 1220) {
                if ($(".box_widget.top").length) {
                    $(".box_widget.top").hide();

                    $(".to-top-icons").css("bottom", "10px");
                    $("#chat-application").removeClass("bottom-add-cart");

                    $(".box_widget.top").css("bottom", "-75px");
                    $(".box_widget.top").css("top", "unset");
                }
            }
        }

        if ($(this).scrollTop() > 150) {
            if ($(".prefilters").length && $('#filtersModal .modal-body .extended_filters_category').length > 0) {
                $(".to-top-filters").show();
            }

            $(".to-top-icons").addClass("show");
            $(".main-header, #mainmenu").addClass("fixed");

            /* sticky breadcrump */
            if ($("#breadcrumb_interactive").length) {
                $("body").addClass("keepfixedwithbread");
            } else {
                $("body").addClass("keepfixed");
            }

            /* sticky breadcrump for product info */
            if ($(".product-main-row").length) {
                $("body").addClass("keepfixedproduct");
            } else {
                $("#breadcrumb_interactive").addClass("fixed");
            }
        } else {
            $(".to-top-icons").removeClass("show");
            $(".main-header, #mainmenu, #breadcrumb_interactive").removeClass(
                "fixed"
            );
            $("body").removeClass("keepfixed");
            $("body").removeClass("keepfixedwithbread");
            $("body").removeClass("keepfixedproduct");
            $(".mainmenu").removeClass("show");
            $(".sticky-menu-toggler").removeClass("clicked");
            $(".menuholder").removeClass("hide");
        }
    });

    if ($(window).width() < 1024) {
        if ($(".outer-filters-wrap").length) {
            $(".to-top-icons").addClass("showit");
        }
    }

    $(window).scroll(function () {
        if ($(window).scrollTop() + $(window).height() == $(document).height()) {
            $('.to-top-icons').addClass('bottom');
            $('#chat-application').addClass('bottom');
        }
        else {
            $('.to-top-icons').removeClass('bottom');
            $('#chat-application').removeClass('bottom');
        }
    });

    $("#return-to-top").click(function () {
        $("body,html").animate(
            {
                scrollTop: 0,
            },
            500
        );
    });

    $(".go-to-availability").click(function () {
        $(".shops-availability-box").removeClass("blink_me");

        $("body,html").animate(
            {
                scrollTop: $(".shops-availability-box").offset().top - 200,
            },
            500
        );

        setTimeout(function () {
            $(".shops-availability-box").addClass("blink_me");
        }, 500);

        return false;
    });

    if ($(".attribute_select").length) {
        $(function () {
            var cc = 0;
            $(".attribute_select")
                .click(function () {
                    $(this).change();
                })
                .change(function () {
                    var product_id = $("input[name=products_id]").val();
                    var option_id = $(this).attr("data-id");
                    var option_value_id = $(this).attr("data-value");
                    var image_div = $(document).find(
                        ".productinfo .productphotogallery .image"
                    );
                    $(image_div).css("min-height", $(image_div).height());
                    $.ajax({
                        type: "POST",
                        url: "../update_attribute_image.php",
                        data: {
                            products_id: product_id,
                            options_id: option_id,
                            options_values_id: option_value_id,
                        },
                        success: function (response) {
                            if (response) {
                                //$(document).find('.productinfo .productphotogallery .image a').replaceWith(response);
                                $(document)
                                    .find(
                                        ".product-c-1 .product-gallery .image a"
                                    )
                                    .replaceWith(response);

                                /*CloudZoom.quickStart();
                            $(function(){
                                $('#productzoom').bind('click',function(){ 
                                    var cloudZoom = $(this).data('CloudZoom'); 
                                    cloudZoom.closeZoom();
                                    $('#main_product_image_link').trigger('click');
                                    return false;
                                });
                            });*/
                            }
                        },
                    });

                    $.ajax({
                        type: "POST",
                        url: "../update_attribute_price.php",
                        data: {
                            products_id: product_id,
                            options_id: option_id,
                            options_values_id: option_value_id,
                        },
                        success: function (response) {
                            if (response && response.length > 0) {
                                $(".productprice #value").html(response);
                            }
                        },
                    });
                    cc = -1;
                });
        });
    }
    $(document).mouseup(function (e) {
        var container = $(".product_quickview");

        if (!container.is(e.target) && container.has(e.target).length === 0) {
            container.parent().removeClass("active");
        }
    });
    if ($(".new_widget_banner_container").length) {
        $(".new_widget_banner i").click(function () {
            $(this).parents().eq(1).hide();
        });
    }

    /*this is for product request from wishlist*/
    $("#request_prod_wishlist").click(function (e) {
        e.preventDefault();
        $("#wishlist_form").append(
            $("<input/>")
                .attr("type", "hidden")
                .attr("name", "request_prod")
                .attr("value", "request_prod")
        );
        $("#wishlist_form").submit();
    });
