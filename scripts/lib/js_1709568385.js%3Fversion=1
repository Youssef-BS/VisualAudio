        dots: true,
        infinite: true,
        speed: 1000,
        slidesToShow: 1,
        slidesToScroll: 1,
        pauseOnHover: false,
        pauseOnFocus: false,
        autoplay: true,
        autoplaySpeed: 4500,
        responsive: [
            {
              breakpoint: 767,
              settings: {
                autoplay: false,
              }
            },
        ]
        
    });

    $(".jsFeaturedProductsHome").slick({
        arrows: true,
        dots: true,
        infinite: true,
        speed: 500,
        slidesToShow: 1,
        autoplay: true,
        autoplaySpeed: 4000,
        cssEase: 'linear'
    });

    if ($("#attributes_combinations_quantities").length) {
        var obj = JSON.parse($("#attributes_combinations_quantities").html());
    }
    initializeAttributesCombinations();
    $(document).on("change", ".attributes-combination-action", function () {
        var category_id = $(this).attr("data-attribute-category");
        $(
            '.attributes-combination-action:not([data-attribute-category="' +
            category_id +
            '"])'
        ).attr("disabled", true);
        $(
            '.attributes-combination-action:not([data-attribute-category="' +
            category_id +
            '"])'
        )
            .closest(".radio-holder")
            .addClass("inactive");
        attributesCombinationsManager(
            attributes_combinations,
            getSelectedAttributes()
        );
        $(".attributes-combination-action:checked").addClass("imClicked");
        $(".attributes-combination-action:checked")
            .closest(".radio-holder")
            .addClass("parentSelected");
        $(".cart_quantity_value").trigger("blur");
        handleClearSelections();
        updateAttributePrice();
    });
    $(document).on("click", ".attributes-combination-action", function () {
        if ($(this).hasClass("imClicked")) {
            $(this).prop("checked", false);
            $(this).removeClass("imClicked");
            $(this).closest(".radio-holder").removeClass("parentSelected");
            $(this).trigger("change");
            /*trigger change event of the last changed attribute category*/
            var category_sort_value = $(this).attr("data-category-sort");
            var next_category = parseInt(category_sort_value) + 1;
            var prev_category = parseInt(category_sort_value) - 1;
            var el;
            if (
                $(
                    '.attributes-combination-action[data-category-sort="' +
                    next_category +
                    '"]:checked'
                ).length
            ) {
                el = $(
                    '.attributes-combination-action[data-category-sort="' +
                    next_category +
                    '"]:checked'
                );
            } else if (
                $(
                    '.attributes-combination-action[data-category-sort="' +
                    prev_category +
                    '"]:checked'
                ).length
            ) {
                el = $(
                    '.attributes-combination-action[data-category-sort="' +
                    prev_category +
                    '"]:checked'
                );
            } else {
                el = $(".attributes-combination-action:checked");
            }
            el.prop("checked", false);
            el.trigger("change");
            el.prop("checked", true);
            el.trigger("change");
        } else {
            $(".attributes-combination-action").removeClass("imClicked");
            $(".attributes-combination-action").each(function () {
                $(this).closest(".radio-holder").removeClass("parentSelected");
            });

            $(this).addClass("imClicked");
            $(this).closest(".radio-holder").addClass("parentSelected");
        }
        updateProductImage($(this));
    });
    $(document).on("click", ".clear-selections", function () {
        clearSelections();
    });
    if (
        $(".parentSelected").length == 0 &&
        $(".attributes-combination-action").length > 0
    ) {
        clearSelections();
    }
    if ($(".attributes-combination-action").length > 0) {
        triggerAttributesCombinations();
        initializeProductInfoAttributes();
    }

    /* attributes dropdown functions NEW */
    $(document).on("click", ".pfield-selected-action", function () {
        var dropId = $(this).attr("data-drop-id");
        var dropHeight = $(this).parents(".col").outerHeight();

        if (
            $(this)
                .parent(".pfield-wrap")
                .children('.pfield[data-id="' + dropId + '"]')
                .hasClass("opened")
        ) {
            $(this).removeClass("clicked");
            $(".pfield-wrap .pfield").removeClass("opened");
        } else {
            $(".pfield-selected-action").removeClass("clicked");
            $(this).addClass("clicked");

            $(".pfield-wrap .pfield").removeClass("opened");
            $('.pfield-wrap .pfield[data-id="' + dropId + '"]').addClass(
                "opened"
            );
            $('.pfield-wrap .pfield[data-id="' + dropId + '"]').css(
                "top",
                dropHeight
            );
        }
    });

    /* click on attribute actions */
    $(document).on(
        "click",
        ".pfield-wrap .pfield .radio-holder label",
        function () {
            var clickedHtml = $(this).html();
            var theLabel = $(this);

            if (
                $(theLabel).parents(".radio-holder").hasClass("parentSelected")
            ) {
                $(theLabel)
                    .parents(".pfield-wrap")
                    .find(".selected-field")
                    .addClass("d-none");
                $(theLabel)
                    .parents(".pfield-wrap")
                    .find(".selected-field")
                    .html("");
                $(theLabel)
                    .parents(".pfield-wrap")
                    .find(".default-field")
                    .removeClass("d-none");
            } else {
                $(theLabel)
                    .parents(".pfield-wrap")
                    .find(".selected-field")
                    .html(clickedHtml);
                $(theLabel)
                    .parents(".pfield-wrap")
                    .find(".selected-field")
                    .removeClass("d-none");
                $(theLabel)
                    .parents(".pfield-wrap")
                    .find(".default-field")
                    .addClass("d-none");
                $(theLabel)
                    .parents(".pfield-wrap")
                    .find(".pfield.opened")
                    .removeClass("opened");
                $(".pfield-selected-action").removeClass("clicked");
            }
        }
    );

    /* check if there is only one attribute */
    if ($(".pfield-selected-action").length) {
        $(".pfield-wrap").each(function () {
            var countAttr = $(this).find(".inner-wrap").children().length;
            var thatWrap = $(this);

            if (countAttr <= 1) {
                var attrHtml = $(thatWrap).find(".radio-holder label").html();
                $(thatWrap).find(".selected-field").html(attrHtml);
                $(thatWrap).find(".selected-field").removeClass("d-none");
                $(thatWrap).find(".default-field").addClass("d-none");
            }
        });
    }
    /* check if there is only one attribute inside quickview modal */
    $("#quickviewModal").on("shown.bs.modal", function () {
        if ($(".pfield-selected-action").length) {
            $(".pfield-wrap").each(function () {
                var countAttr = $(this).find(".inner-wrap").children().length;
                var thatWrap = $(this);

                if (countAttr <= 1) {
                    var attrHtml = $(thatWrap)
                        .find(".radio-holder label")
                        .html();
                    $(thatWrap).find(".selected-field").html(attrHtml);
                    $(thatWrap).find(".selected-field").removeClass("d-none");
                    $(thatWrap).find(".default-field").addClass("d-none");
                } else {
                    /* disable quickview buttons before user selects attributes */
                    $("#quickviewModal .quickview-buttons button").prop(
                        "disabled",
                        true
                    );
                }
            });
        }
    });

    /* clear selection extra functions */
    $(document).on(
        "click",
        ".clear-selections-actions .clear-selections",
        function () {
            $(".pfield-wrap .selected-field").addClass("d-none");
            $(".pfield-wrap .selected-field").html("");
            $(".pfield-wrap .default-field").removeClass("d-none");
        }
    );

    /* close attribute dropdown if click outside or on the action button*/
    $(document).mouseup(function (e) {
        var attrDropdown = $(".pfield-wrap .pfield, .pfield-selected-action");
        if (
            !attrDropdown.is(e.target) &&
            attrDropdown.has(e.target).length === 0
        ) {
            attrDropdown.removeClass("opened");
            $(".pfield-selected-action").removeClass("clicked");
        }
    });
    /* use .lazy-scroll class on images*/
    if ($(".lazy-scroll")[0]) {
        /* lazy load on first page load */
        if ($(".lazy-scroll").isInViewport()) {
            $(".lazy-scroll").each(function () {
                var $this = $(this);
                var src = $this.attr("data-lazy-src");
                $this.attr("src", src);
                $this.removeAttr("data-lazy-src");
            });
        }
        /* lazy load on scroll */
        $(window).on("scroll", function () {
            if ($(".lazy-scroll").isInViewport()) {
                $(".lazy-scroll").each(function () {
                    var $this = $(this);
                    var src = $this.attr("data-lazy-src");
                    $this.attr("src", src);
                    $this.removeAttr("data-lazy-src");
                });
            }
        });
    }

    /* use .lazy-scroll-bg class on elements for background image attribute */
    if ($(".lazy-scroll-bg")[0]) {
        /* lazy load on first page load */
        if ($(".lazy-scroll-bg").isInViewport()) {
            $(".lazy-scroll-bg").each(function () {
                if (this.hasAttribute("data-lazy-src")) {
                    var $this = $(this);
                    var src = $this.attr("data-lazy-src");
                    $this.css("background-image", "url(" + src + ")");
                    $this.removeAttr("data-lazy-src");
                }
            });
        }
        /* lazy load on scroll */
        $(window).on("scroll", function () {
            if ($(".lazy-scroll-bg").isInViewport()) {
                $(".lazy-scroll-bg").each(function () {
                    if (this.hasAttribute("data-lazy-src")) {
                        var $this = $(this);
                        var src = $this.attr("data-lazy-src");
                        $this.css("background-image", "url(" + src + ")");
                        $this.removeAttr("data-lazy-src");
                    }
                });
            }
        });
    }

    $(document).on("click", ".ajax_get_subcategories", function () {
        var action_element = $(this);
        if (action_element.val() !== "") {
            var category_id = action_element.val();
            var depth = action_element.attr("data-depth");
            var next_depth = parseInt(depth) + 1;
            while (
                $('.ajax_get_subcategories[data-depth="' + next_depth + '"]')
                    .length > 0
            ) {
                $(
                    '.ajax_get_subcategories[data-depth="' + next_depth + '"]'
                ).remove();
                next_depth++;
            }
            if (category_id !== undefined && depth !== undefined) {
                var formdata = { category_id: category_id, depth: depth };
                $.ajax({
                    type: "POST",
                    url: "ajax_get_categories_dropdown.php",
                    data: formdata,
                    success: function (response) {
                        if (response !== "") {
                            $(".ajax_get_subcategories").prop("name", "");
                            $(".ajax-categories .fieldkey").append(response);
                        }
                    },
                });
            }
        }
    });

    /* open fullscreen search */
    $(".search-toggle").on("click", function () {

        if ($('.acresults.search--open').length)
        {
            $('.acresults').hide();
            $("body").removeClass("no-scroll");
            $('.acresults').removeClass('search--open');
        }
        else
        {
            $("#acresults").css('display', 'block');
            $("body").addClass("no-scroll");
            $('.acresults').addClass('search--open');
        }

        return false;
    });

    /* sticky search */
    $(".search-container .inner-wrap").on("scroll", function () {
        if ($(this).scrollTop() > 200) {
            $(".search-container").addClass("fixed-search");
            $(".search-title").addClass("hide-for-sticky");
        } else {
            $(".search-container").removeClass("fixed-search");
            $(".search-title").removeClass("hide-for-sticky");
        }
    });

    /* open minicart function */
    $(".shoppingcart.toggler").click(function () {
        if ($(this).find("#shopping-cart-items-container").hasClass("active")) {
            $("#shopping-cart-items-container").removeClass("active");
        } else {
            $("#shopping-cart-items-container").addClass("active");
        }
    });
    /* dont close mini cart when clicked inside */
    $(document).on("click", "#shopping-cart-items-container", function () {
        $("#shopping-cart-items-container").addClass("active");
    });
    /*close mini cart when clicked outside*/
    $(document).on("click", ".mainbody, .search-container", function () {
        $("#shopping-cart-items-container").removeClass("active");
    });

    // image pin slider mobile fix
    $(document).on("click", ".imapper1-pin", function () {
        var thisEl = $(this);
        if ($(thisEl).is(".imapper-no-overlay")) {
            $(thisEl).parents('.imapper-pin-wrapper').addClass('clicked');
        }else{
            $(thisEl).parents('.imapper-pin-wrapper').removeClass('clicked');
        }
    });

