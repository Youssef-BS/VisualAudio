    /* categories gallery perfect square boxes */
    $(".category-box.square, .home-categories .box").each(function () {
        $(this).outerHeight($(this).outerWidth());
    });

    $(".category-box.large").each(function () {
        $(this).outerHeight($(this).outerWidth() + 10);
    });

    $(window).on("resize", function () {
        $(".category-box.square, .home-categories .box").each(function () {
            $(this).outerHeight($(this).outerWidth());
        });

        $(".category-box.large").each(function () {
            $(this).outerHeight($(this).outerWidth() + 10);
        });
    });

    $(document).on("click", ".action-button", function () {
        if (!$(this).hasClass("active")) {

            var el = $(this).closest(".ajax-carousel-container");

            addLoader(el);

            el.find(".action-button").removeClass("active");
            $(this).addClass("active");
            var action = $(this).attr("data-action");
            var category_id = $(this).attr("data-category-id");

            var formdata = {
                action: "switch_carousel",
                data: action,
                category_id: category_id,
                language: $("#language").val(),
            };

            $.ajax({
                type: "POST",
                url: "ajax_update_carousel.php",
                data: formdata,
                success: function (response) {
                    var obj = JSON.parse(response);
                    destroyCarousel();
                    el.find(".carousel-container").replaceWith(obj.carousel);
                    el.find(".view-all-cat").attr("href", obj.link);
                    /* el.find(".ajax-carousel-desc").replaceWith(obj.description); */
                    initializeSlick();
                    removeLoader(el);
                    lazyProductBox();
                },
            });
        }
    });
});

function ajaxRefreshShoppingCart(action, origin, el) {
    if (origin == "mini-shopping-cart" || origin == "product-listing") {
        var formData;
        var has_attr;
        if (action == "add_product_from_listing") {
            if (
                parseInt(
                    $(".update-shopping-cart.activated").attr("data-multiples")
                ) > 0
            ) {
                var default_quantity = $(
                    ".update-shopping-cart.activated"
                ).attr("data-multiples");
            } else {
                var default_quantity = 1;
            }
            var listing_products_id = $(".update-shopping-cart.activated").attr(
                "data-product-id"
            );
            has_attr = $(".update-shopping-cart.activated").attr(
                "data-has-attr"
            );
            formData = {
                cart_quantity: default_quantity,
                products_id: listing_products_id,
                action: action,
            };
        } else {
            var minicart_quantity = [];
            $(".minicart_quantity_action").each(function (index, element) {
                minicart_quantity.push($(this).val());
            });
            var miniproducts_id = [];
            $('input[type="hidden"][name="minicart_products_id[]"]').each(
                function (index, element) {
                    miniproducts_id.push($(this).val());
                }
            );
            var minicart_delete = [];
            $("#shopping-cart-items-container .item").each(function (
                index,
                element
            ) {
                if (
                    $(this)
                        .find('input[name="minicart_delete[]"]')
                        .is(":checked")
                ) {
                    minicart_delete.push(
                        $(this).find('input[name="minicart_delete[]"]').val()
                    );
                }
            });
            var id = {};
            $(".products_attributes_values_array_minicart").each(function (
                index,
                element
            ) {
                var key = $(this).attr("name");
                var val = $(this).val();
                id[key] = val;
            });

            formData = {
                cart_quantity: minicart_quantity,
                products_id: miniproducts_id,
                cart_delete: minicart_delete,
                action: action,
                id: id,
            };
        }
        $.ajax({
            url: "ajax_update_shopping_cart.php",
            type: "post",
            data: formData,
            success: function (response) {
                var obj = JSON.parse(response);
                if (obj["redirect"] != "" && obj["redirect"] != null) {
                    window.location = obj["redirect"];
                } else {
                    if ($("#shopping_cart_dynamic_content").length) {
                        $("#shopping_cart_dynamic_content").html(
                            obj["shopping_cart"]
                        );
                    }
                    if ($("#mini-shopping-cart-details-dynamic").length) {
                        $("#mini-shopping-cart-details-dynamic").replaceWith(
                            obj["shopping_cart_mini_toggler"]
                        );
                    }

                    if ($("#shopping-cart-items-wrapper").length) {
                        $("#shopping-cart-items-wrapper").replaceWith(
                            obj["shopping_cart_mini_container"]
                        );
                    }

                    var elementOffsetY = localStorage.getItem("elementOffsetY");
                    $("#mini-cart-panel").scrollTop(elementOffsetY);
                    localStorage.removeItem("elementOffsetY");

                    if (
                        action == "add_product_from_listing" &&
                        !$("#shopping-cart-items-container").hasClass("active")
                    ) {
                        $("#shopping-cart-items-container").addClass("active");
                    }
                }
            },
            error: function (xhr, desc, err) {
                console.log(xhr);
                console.log("Details: " + desc + "\nError:" + err);
            },
        });
        $(".update-shopping-cart").removeClass("activated");
    } else if (origin == "product-page") {
        var cart_quantity = $("#cart_quantity").val();
        var products_id = $('input[type="hidden"][name="products_id"]').val();
        var id = {};
        $(".productattributes input[type=radio]:checked").each(function (
            index,
            element
        ) {
            var key = $(this)
                .attr("name")
                .replace(
                    /\D+/g,
                    ""
                ); /*replace all leading non-digits with nothing;*/
            var val = $(this).val();
            id[key] = val;
        });
        var buy_also_checkboxes = [];
        var buyalso_quantity = [];
        var buyalso_quantity_var_name = [];
        $('input[name="buy_also_checkboxes[]"]').each(function (
            index,
            element
        ) {
            if ($(this).is(":checked")) {
                var buy_also_id = $(this).val();
                buy_also_checkboxes.push(buy_also_id);
                buyalso_quantity.push(
                    $(
                        'input[name="buyalso_quantity_' + buy_also_id + '"]'
                    ).val()
                );
                buyalso_quantity_var_name.push(
                    "buyalso_quantity_" + buy_also_id
                );
            }
        });
        var formData = {
            cart_quantity: cart_quantity,
            products_id: products_id,
            cart_delete: cart_delete,
            action: action,
            id: id,
            buy_also_checkboxes: buy_also_checkboxes,
        };
        for (var count = 0; count < buyalso_quantity_var_name.length; count++) {
            formData[buyalso_quantity_var_name[count]] =
                buyalso_quantity[count];
        }
        $.ajax({
            url: "ajax_update_shopping_cart.php",
            type: "post",
            data: formData,
            success: function (response) {
                var obj = JSON.parse(response);
                if (obj["redirect"] != "" && obj["redirect"] != null) {
                    window.location = obj["redirect"];
                } else {
                    if ($("#shopping_cart_dynamic_content").length) {
                        $("#shopping_cart_dynamic_content").html(
                            obj["shopping_cart"]
                        );
                    }
                    if ($("#mini-shopping-cart-details-dynamic").length) {
                        $("#mini-shopping-cart-details-dynamic").replaceWith(
                            obj["shopping_cart_mini_toggler"]
                        );
                    }
                    if ($("#shopping-cart-items-wrapper").length) {
                        $("#shopping-cart-items-wrapper").replaceWith(
                            obj["shopping_cart_mini_container"]
                        );
                    }

                    var elementOffsetY = localStorage.getItem("elementOffsetY");
                    $("#mini-cart-panel").scrollTop(elementOffsetY);
                    localStorage.removeItem("elementOffsetY");

                    if (
                        !$("#shopping-cart-items-container").hasClass("active")
                    ) {
                        $("#shopping-cart-items-container").addClass("active");
                    }
                }
            },
            error: function (xhr, desc, err) {
                console.log(xhr);
                console.log("Details: " + desc + "\nError:" + err);
            },
        });
    } else if (origin == "product-quickview") {
        var myform = $("#quickview_cart_quantity");
        var cart_quantity = myform.find(".cart_quantity_value").val();

        var products_id = myform
            .find('input[type="hidden"][name="quickview_products_id"]')
            .val();
        var id = {};
        var attribute_select = myform.find(".attribute_select");
        var attribute_key = attribute_select.attr("data-id");
        var attribute_val = attribute_select.val();
        id[attribute_key] = attribute_val;

        var formData = {
            cart_quantity: cart_quantity,
            products_id: products_id,
            cart_delete: cart_delete,
            action: action,
            id: id,
        };
        $.ajax({
            url: "ajax_update_shopping_cart.php",
            type: "post",
            data: formData,
            success: function (response) {
                var obj = JSON.parse(response);
                if (obj["redirect"] != "" && obj["redirect"] != null) {
                    window.location = obj["redirect"];
                } else {
                    if ($("#shopping_cart_dynamic_content").length) {
                        $("#shopping_cart_dynamic_content").html(
                            obj["shopping_cart"]
                        );
                    }
                    if ($("#mini-shopping-cart-details-dynamic").length) {
                        $("#mini-shopping-cart-details-dynamic").replaceWith(
                            obj["shopping_cart_mini_toggler"]
                        );
                    }

                    if ($("#shopping-cart-items-wrapper").length) {
                        $("#shopping-cart-items-wrapper").replaceWith(
                            obj["shopping_cart_mini_container"]
                        );
                    }

                    var elementOffsetY = localStorage.getItem("elementOffsetY");
                    $("#mini-cart-panel").scrollTop(elementOffsetY);
                    localStorage.removeItem("elementOffsetY");

                    if (
                        !$("#shopping-cart-items-container").hasClass("active")
                    ) {
                        $("#shopping-cart-items-container").addClass("active");
                    }
                }
            },
            error: function (xhr, desc, err) {
                console.log(xhr);
                console.log("Details: " + desc + "\nError:" + err);
            },
        });
    } 
    else if(origin == 'wishlist-form')
	{
        //console.log("here");
        var itemContainer = el.closest('.productWishlistCart');
        var cart_quantity = itemContainer.find('.wishlist_quantity_value').val();
		var products_id = el.attr('data-product-id');
		var id = {}; 
        
        var formData = {
            cart_quantity: cart_quantity,
            products_id: products_id,
            action: action,
            id: id,
        };
		$.ajax({
			url: 'ajax_update_shopping_cart.php',
			type: 'post',
			data: formData,
			success: function(response) {
                //console.log("response"); 
                //console.log(response); 
				var obj = JSON.parse(response);        
				if(obj['redirect'] != "" && obj['redirect'] != null)
				{
					window.location = obj['redirect'];
				}
				else
				{
          
                    if($('#shopping_cart_dynamic_content').length){
                        $('#shopping_cart_dynamic_content').html(obj['shopping_cart']);
                    }
                    if($('#mini-shopping-cart-details-dynamic').length){            
                        $('#mini-shopping-cart-details-dynamic').replaceWith(obj['shopping_cart_mini_toggler']);
                    }
                    if($('#shopping-cart-items-wrapper').length){
                        $('#shopping-cart-items-wrapper').replaceWith(obj['shopping_cart_mini_container']);
                    }

                  var elementOffsetY = localStorage.getItem("elementOffsetY");
                  $('#mini-cart-panel').scrollTop(elementOffsetY);
                  localStorage.removeItem("elementOffsetY");
          
					if(!$("#shopping-cart-items-container").hasClass('active'))	{
						$("#shopping-cart-items-container").addClass('active');
					}
				}
                $('.update-wishlist.refresh').trigger('click');
			},
			error: function(xhr, desc, err) {
			}
		});
	}    
	else if(origin == 'wishlist-form-add-all')
	{
        var formEl = $('#wishlist_form').length > 0 ? $('#wishlist_form') : $('#mini-wishlist-form');
        
		var products_id = [];
        var cart_quantity = [];
		var id = [];
        
        formEl.find('.productWishlistCart').each(function(){
            var product_id = $(this).find('input[type="hidden"][name="products_id[]"]').val();
            var product_quantity = $(this).find('.wishlist_quantity_value').val();
            
            products_id.push(product_id);
            cart_quantity.push(product_quantity);
        
            var temp_id = {};
            
            $(this).find('.attribute-value').each(function(index, element) {
                var option_id;
                var option_value_id = $(this).val();

                var matches = $(this).attr('name').match(/\[(.*?)\]/);

                if (matches) {
                    var option_id = matches[1];
                    temp_id[option_id] = option_value_id;
                }

            }); 
            id.push(temp_id);
            
            
        });
        
        var formData = {cart_quantity:cart_quantity, products_id:products_id, action:action, id:id};
        
		$.ajax({
			url: 'ajax_update_shopping_cart.php',
			type: 'post',
			data: formData,
			success: function(response) {
                
				var obj = JSON.parse(response);        
				if(obj['redirect'] != "" && obj['redirect'] != null)
				{
					window.location = obj['redirect'];
				}
				else
				{
          
                    if($('#shopping_cart_dynamic_content').length){
                        $('#shopping_cart_dynamic_content').html(obj['shopping_cart']);
                    }
                    if($('#mini-shopping-cart-details-dynamic').length){            
                        $('#mini-shopping-cart-details-dynamic').replaceWith(obj['shopping_cart_mini_toggler']);
                    }
                    if($('#shopping-cart-items-wrapper').length){
                        $('#shopping-cart-items-wrapper').replaceWith(obj['shopping_cart_mini_container']);
                    }

                  var elementOffsetY = localStorage.getItem("elementOffsetY");
                  $('#mini-cart-panel').scrollTop(elementOffsetY);
                  localStorage.removeItem("elementOffsetY");
          
					if(!$("#shopping-cart-items-container").hasClass('active'))	{
						$("#shopping-cart-items-container").addClass('active');
					}
				}
                $('.update-wishlist.refresh').trigger('click');
			},
			error: function(xhr, desc, err) {
			}
		});
	}
    else if (origin == "product-set") {
        var myform = $(".product_sets").find(".cart_quantity_form.active");
        var cart_quantity = myform.find(".set-add-quantity").val();

        var products_id = myform
            .find('input[type="hidden"][name="set_products_id"]')
            .val();

        var id = {};
        if (myform.find(".set_product_attribute :selected").length > 0) {
            myform.find(".set_product_attribute").each(function (e) {
                var atribute_value = $(this).val();

                var res = atribute_value.split("_");
                id[res[0]] = res[1];
            });
        }
        var formData = {
            cart_quantity: cart_quantity,
            products_id: products_id,
            cart_delete: cart_delete,
            action: action,
            id: id,
            language: language,
        };
        $.ajax({
            url: "ajax_update_shopping_cart.php",
            type: "post",
            data: formData,
            success: function (response) {
                var obj = JSON.parse(response);
                if (obj["redirect"] != "" && obj["redirect"] != null) {
                    window.location = obj["redirect"];
                } else {
                    if ($("#shopping_cart_dynamic_content").length) {
                        $("#shopping_cart_dynamic_content").html(
                            obj["shopping_cart"]
                        );
                    }
                    if ($("#mini-shopping-cart-details-dynamic").length) {
                        $("#mini-shopping-cart-details-dynamic").replaceWith(
                            obj["shopping_cart_mini_toggler"]
                        );
                    }

                    if ($("#shopping-cart-items-wrapper").length) {
                        $("#shopping-cart-items-wrapper").replaceWith(
                            obj["shopping_cart_mini_container"]
                        );
                    }

                    if (
                        !$("#shopping-cart-items-container").hasClass("active")
                    ) {
                        $("#shopping-cart-items-container").addClass("active");
                    }
                }
            },
            error: function (xhr, desc, err) { },
        });
    } else {
        var cart_quantity = [];
        $('input[id="cart_quantity[]"').each(function (index, element) {
            cart_quantity.push($(this).val());
        });
        var products_id = [];
        $('input[type="hidden"][name="products_id[]"]').each(function (
            index,
            element
        ) {
            products_id.push($(this).val());
        });
        var cart_delete = [];
        $(".productCart").each(function (index, element) {
            if ($(this).find('input[name="cart_delete[]"]').is(":checked")) {
                cart_delete.push(
                    $(this).find('input[name="cart_delete[]"]').val()
                );
            }
        });
        var id = {};
        $(".products_attributes_values_array").each(function (index, element) {
            var key = $(this).attr("name");
            var val = $(this).val();
            id[key] = val;
        });
        var product_triggered;
        if (el !== undefined) {
            product_triggered = el
                .parent()
                .find('input[type="hidden"][name="products_id[]"]')
                .val();
        } else {
            product_triggered = $(
                'input[type="hidden"][name="products_id"]'
            ).val();
        }
        var formData = {
            cart_quantity: cart_quantity,
            products_id: products_id,
            cart_delete: cart_delete,
            action: action,
            id: id,
            product_triggered: product_triggered,
        };
        $.ajax({
            url: "ajax_update_shopping_cart.php",
            type: "post",
            data: formData,
            success: function (response) {
                var obj = JSON.parse(response);
                if (obj["redirect"] != "" && obj["redirect"] != null) {
                    window.location = obj["redirect"];
                } else {
                    if ($("#shopping_cart_dynamic_content").length) {
                        $("#shopping_cart_dynamic_content").html(
                            obj["shopping_cart"]
                        );
                    }
                    if (
                        $(
                            ".shoppingcart.toggler .shoppingpopup .details.dynamic_content"
                        ).length
                    ) {
                        $(
                            ".shoppingcart.toggler .shoppingpopup .details.dynamic_content"
                        ).replaceWith(obj["shopping_cart_mini_toggler"]);
                    }

                    if (
                        $(
                            "#shopping-cart-items-container .inner-mini-cart.dynamic_content"
                        ).length
                    ) {
                        $(
                            "#shopping-cart-items-container .inner-mini-cart.dynamic_content"
                        ).replaceWith(obj["shopping_cart_mini_container"]);
                    }
                    var elementOffsetY = localStorage.getItem("elementOffsetY");
                    $("#mini-cart-panel").scrollTop(elementOffsetY);
                    localStorage.removeItem("elementOffsetY");
                }
                setTimeout(function () {
                    $(".max-quantity-warning").fadeOut("fast");
                }, 2000);
            },
            error: function (xhr, desc, err) {
                console.log(xhr);
                console.log("Details: " + desc + "\nError:" + err);
            },
        });
    }
}
/*Custom Autocomplete Functions*/
function preg_quote(str) {
    return (str + "").replace(
        /([\\\.\+\*\?\[\^\]\$\(\)\{\}\=\!\<\>\|\:])/g,
        "\\$1"
    );
}
function highlight(data, search) {
    return data.replace(
        new RegExp("(" + preg_quote(search) + ")", "gi"),
        "<b>$1</b>"
    );
}
