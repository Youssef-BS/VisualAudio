    $(".side-menu.main-menu-container").on("click", function (e) {
        var container = $(".main-menu-container .inner");

        // if the target of the click isn't the container nor a descendant of the container
        if (!container.is(e.target) && container.has(e.target).length === 0) {
            $("a.menu-open").click();
        }
    });

    $(".menu-open").on("click", function () {

        //fix second level scroll height
        if ($(".main-header").hasClass("fixed")) {
            var bodyHeight = $(window).outerHeight();
            var headerHeight = $('.top-bar').outerHeight();
            var secondLevelHeight = bodyHeight - headerHeight -30;
            $('.main-menu-container .first-level ul ul.second-level .inner-second, .main-menu-container .first-level ul ul.second-level .third-level .inner-third, .main-menu-container .first-level ul ul.second-level .third-level .inner-third .inner-fourth').css('max-height', secondLevelHeight);
        }else {
            var bodyHeight = $(window).outerHeight();
            var headerHeight = $('.top-bar').outerHeight();
            var topBarHeight = $('.main-header').outerHeight();
            var secondLevelHeight = bodyHeight - headerHeight - topBarHeight -30;
            $('.main-menu-container .first-level ul ul.second-level .inner-second, .main-menu-container .first-level ul ul.second-level .third-level .inner-third, .main-menu-container .first-level ul ul.second-level .third-level .inner-third .inner-fourth').css('max-height', secondLevelHeight);
        }

        if ($(".acresults").is(":visible") && $('.side-menu').hasClass("hide")) {
            $('.search-toggle').trigger("click");
        }

        $(".not-found").removeClass("disable");
        $(".search-trigger").removeClass("clicked");
        $(".overlay-slidedown").removeClass("open");

        if ($("body").hasClass("no-scroll")) {
            $("body").removeClass("no-scroll");
        } else {
            $("body").addClass("no-scroll");
        }
        //fix small bug if header is almost sticky
        if ($("body").hasClass("keepfixed")) {
        } else {
            $('html, body').animate({ scrollTop: 0 }, 400);
        }

        $(this).toggleClass("clicked");
        $(".side-menu").toggleClass("hide");
        $(".side-menu ul.second-level").hide();
        $(".side-menu li.active").removeClass("active");

        setTimeout(function () {
            $(".jsFirstLevelMenuItem:first-child").trigger('mouseenter');
            $(".jsFirstLevelMenuItem:first-child").addClass('active');
            $(".jsFirstLevelMenuItem:first-child .second-level").css('display', 'none');
        }, 100);

        setTimeout(function () {
            $(".jsFirstLevelMenuItem:first-child .second-level").fadeIn('50');
        }, 240);
        return false;

        
    });

