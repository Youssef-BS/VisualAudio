        $(".main-menu-container .menu-item").on('click', function (event) {
            var container = $(this).closest('ul');
            if ($(this).hasClass('second-level-container')) {
                $('.second-level-container.active').removeClass('active');
            }
            else if ($(this).hasClass('third-level-container')) {
                $('.third-level-container.active').removeClass('active');
            }
            else if ($(this).hasClass('fourth-level-container')) {
                $('.fourth-level-container.active').removeClass('active');
            }
    
            container.find(".second-level").hide();
            container.find(".third-level").hide();
            container.find(".fourth-level").hide();
    
            var second_level = $(this).find(".second-level");
    
            $(this).addClass("active");
    
            if ($(this).find('.third-level-container.active').length === 0) {
                $(this).find('.first-option.third-level-container').addClass('active');
            }
            if ($(this).find('.fourth-level-container.active').length === 0) {
                $(this).find('.first-option.fourth-level-container').addClass('active');
            }
    
            var third_level = $(this).find(".third-level-container.active .third-level");
            if (third_level.length === 0) {
                third_level = $(this).find(".third-level");
            }
            var fourth_level = $(this).find(".fourth-level-container.active .fourth-level");
            if (fourth_level.length === 0) {
                fourth_level = $(this).find(".fourth-level");
            }
    
            if (second_level.length) {
                second_level.show();
            }
    
            if (third_level.length) {
                third_level.show();
            }
    
            if (fourth_level.length) {
                fourth_level.show();
            }
    
            if ($(this).find('.active .active .active .active .menu-products-container').length > 0) {
                $(this).find('.active .active .active .active .menu-products-container img').each(function () {
    
                    var data_src = $(this).attr('data-src');
                    if (data_src !== undefined && data_src !== '') {
                        $(this).attr('src', data_src);
                        $(this).removeAttr('data-src');
                    }
    
                });
            }
            else if ($(this).find('.active .active .active .menu-products-container').length > 0) {
                $(this).find('.active .active .active .menu-products-container img').each(function () {
    
                    var data_src = $(this).attr('data-src');
                    if (data_src !== undefined && data_src !== '') {
                        $(this).attr('src', data_src);
                        $(this).removeAttr('data-src');
                    }
    
                });
            }
            else if ($(this).find('.active .active .menu-products-container').length > 0) {
                $(this).find('.active .active .menu-products-container img').each(function () {
    
                    var data_src = $(this).attr('data-src');
                    if (data_src !== undefined && data_src !== '') {
                        $(this).attr('src', data_src);
                        $(this).removeAttr('data-src');
                    }
    
                });
            }
            else if ($(this).find('.active .menu-products-container').length > 0) {
                $(this).find('.active .menu-products-container img').each(function () {
    
                    var data_src = $(this).attr('data-src');
                    if (data_src !== undefined && data_src !== '') {
                        $(this).attr('src', data_src);
                        $(this).removeAttr('data-src');
                    }
    
                });
            }
            else if ($(this).find('.menu-products-container').length > 0) {
                $(this).find('.menu-products-container img').each(function () {
    
                    var data_src = $(this).attr('data-src');
                    if (data_src !== undefined && data_src !== '') {
                        $(this).attr('src', data_src);
                        $(this).removeAttr('data-src');
                    }
    
                });
            }

            
    
        });

