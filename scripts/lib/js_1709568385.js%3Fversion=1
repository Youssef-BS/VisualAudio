    /* product info columns */
    if ($(".product-main-row").length) {
        /*change product info columns*/
        if ($(document).width() > 1193) {
            $(".product-c-2").insertAfter(".product-c-1");
            $(".product-c-1 .headingtitle").prependTo(".product-c-2");
            $(".product-c-1 .product-title-info").insertAfter(
                ".product-c-2 .headingtitle"
            );
        } else {
            $(".product-c-2 .headingtitle").prependTo(".product-c-1");
            $(".product-c-2 .product-title-info").insertAfter(
                ".product-c-1 .headingtitle"
            );
        }

        $(window).on("resize", function () {
            /*change product info columns on resize*/
            if ($(document).width() > 1193) {
                $(".product-c-2").insertAfter(".product-c-1");
                $(".product-c-1 .headingtitle").prependTo(".product-c-2");
                $(".product-c-1 .product-title-info").insertAfter(
                    ".product-c-2 .headingtitle"
                );
            } else {
                $(".product-c-2 .headingtitle").prependTo(".product-c-1");
                $(".product-c-2 .product-title-info").insertAfter(
                    ".product-c-1 .headingtitle"
                );
            }

            if ( $(".jsProductCarosulFiveProducts .prev-arrow.slick-arrow").length == 0 ) {
                var sliderWrapper = $(".jsProductCarosulFiveProducts").closest('.product-page-section.product-page-section--slider');
                sliderWrapper.removeClass("product-page-section--slider");
            }
        });
    }

    /*Buy also carousel item actions*/
    $(
        ".buy-also-item-actions .item-add, .buy-also-item .buy-also-item-title"
    ).click(function () {
        var itemTitle = $(this)
            .parents(".buy-also-item")
            .find(".buy-also-item-title span.title")
            .text();
        var itemID = $(this).parents(".buy-also-item").attr("data-id");
        var itemQuantity = $(this)
            .parents(".buy-also-item")
            .find(".buy-also-item-actions .item-quantity .quantity-field")
            .val();
        $(this).parents(".buy-also-item").toggleClass("selected");
        /*check if item exist in price container*/
        if ($(".buy-also-items-container .line#" + itemID + "").length) {
            $(".buy-also-items-container .line#" + itemID + "").remove();
        }

        /*change add to cart button text*/
        if ($(".buy-also .buy-also-item").hasClass("selected")) {
            $(this)
                .parents(".product-c-3")
                .find(".shop-btn span")
                .html(
                    "   <span class='lights'>(+ )</span>"
                );
            $(this)
                .parents(".product-c-3")
                .find(".shop-btn")
                .addClass("selected");
            /*add buy also item under price*/
            $(".productprice .buy-also-items-container").addClass("active");
            $(".buy-also-items-container .line#" + itemID + "").remove();
            $(".productprice .buy-also-items-container").append(
                "<div class='line' title='" +
                itemTitle +
                "' id='" +
                itemID +
                "'><span class='add'>+</span>" +
                itemQuantity +
                "<span class='times'>x</span>" +
                itemTitle +
                "</div>"
            );
        } else {
            $(this)
                .parents(".product-c-3")
                .find(".shop-btn span")
                .html("  ");
            $(this)
                .parents(".product-c-3")
                .find(".shop-btn")
                .removeClass("selected");
            /*remove buy also item under price*/
            $(".productprice .buy-also-items-container").removeClass("active");
            $(".productprice .buy-also-items-container").html("");
        }

        /*auto-check items checkbox*/
        if ($(this).parents(".buy-also-item").hasClass("selected")) {
            $(this)
                .parents(".buy-also-item")
                .find("input.buy-also-checkboxes-item")
                .prop("checked", true)
                .triggerHandler("click");
        } else {
            $(this)
                .parents(".buy-also-item")
                .find("input.buy-also-checkboxes-item")
                .prop("checked", false)
                .triggerHandler("click");
            $(".buy-also-items-container .line#" + itemID + "").remove();
        }
    });

    /*Buy also quantity input buttons*/
    function incrementItemValue(e) {
        e.preventDefault();
        var fieldName = $(e.target).data("field");
        var parent = $(e.target).closest(".item-quantity");
        var currentVal = parseInt(
            parent.find("input[name=" + fieldName + "]").val(),
            10
        );

        if (!isNaN(currentVal)) {
            parent.find("input[name=" + fieldName + "]").val(currentVal + 1);
        } else {
            parent.find("input[name=" + fieldName + "]").val(1);
        }
    }

    function decrementItemValue(e) {
        e.preventDefault();
        var fieldName = $(e.target).data("field");
        var parent = $(e.target).closest(".item-quantity");
        var currentVal = parseInt(
            parent.find("input[name=" + fieldName + "]").val(),
            10
        );

        if (!isNaN(currentVal) && currentVal > 1) {
            parent.find("input[name=" + fieldName + "]").val(currentVal - 1);
        } else {
            parent.find("input[name=" + fieldName + "]").val(1);
        }
    }

    $(".buy-also-item-actions .item-quantity").on(
        "click",
        ".button-plus",
        function (e) {
            incrementItemValue(e);
            /*change value and add price line if item is selected*/
            if (
                $(this).parents(".buy-also .buy-also-item").hasClass("selected")
            ) {
                var itemTitle = $(this)
                    .parents(".buy-also-item")
                    .find(".buy-also-item-title")
                    .text();
                var itemID = $(this).parents(".buy-also-item").attr("data-id");
                var itemQuantity = $(this)
                    .parents(".buy-also-item")
                    .find(".item-quantity .quantity-field")
                    .val();

                $(".productprice .buy-also-items-container").addClass("active");
                $(".buy-also-items-container .line#" + itemID + "").remove();
                $(".productprice .buy-also-items-container").append(
                    "<div class='line' title='" +
                    itemTitle +
                    "' id='" +
                    itemID +
                    "'><span class='add'>+</span>" +
                    itemQuantity +
                    "<span class='times'>x</span>" +
                    itemTitle +
                    "</div>"
                );
            }
        }
    );
    $(".buy-also-item-actions .item-quantity").on(
        "click",
        ".button-minus",
        function (e) {
            decrementItemValue(e);

            /*change value and add price line if item is selected*/
            if (
                $(this).parents(".buy-also .buy-also-item").hasClass("selected")
            ) {
                var itemTitle = $(this)
                    .parents(".buy-also-item")
                    .find(".buy-also-item-title")
                    .text();
                var itemID = $(this).parents(".buy-also-item").attr("data-id");
                var itemQuantity = $(this)
                    .parents(".buy-also-item")
                    .find(
                        ".buy-also-item-actions .item-quantity .quantity-field"
                    )
                    .val();

                $(".productprice .buy-also-items-container").addClass("active");
                $(".buy-also-items-container .line#" + itemID + "").remove();
                $(".productprice .buy-also-items-container").append(
                    "<div class='line' title='" +
                    itemTitle +
                    "' id='" +
                    itemID +
                    "'><span class='add'>+</span>" +
                    itemQuantity +
                    "<span class='times'>x</span>" +
                    itemTitle +
                    "</div>"
                );
            }
        }
    );

    /*breadcrump list loading animation*/
    $(document).on(
        "click",
        ".interactive_breadcrumb .container .inner_holder ul li.update_breadcrumb_title",
        function () {
            $(this)
                .parents(".inner_holder")
                .find(".gotolink")
                .addClass("loading");
            $(
                ".interactive_breadcrumb .container:nth-child(4) .gotolink"
            ).removeClass("loading");
            setTimeout(
                "$('.interactive_breadcrumb').find('.gotolink.loading').removeClass('loading');",
                800
            );
        }
    );
    $(document).on("keyup", ".search_breadcrumb", function () {
        if ($(this).val() != "" && $(this).val() != null) {
            $(this).parent().find("i.clear_breadcrumb_search").show();
        } else {
            $(this).parent().find("i.clear_breadcrumb_search").hide();
        }

        var searchString = $(this).val();
        $(this)
            .parent()
            .parent()
            .find(".update_breadcrumb_title")
            .each(function (index, value) {
                currentName = $(value).text();
                if (
                    currentName
                        .toUpperCase()
                        .includes(searchString.toUpperCase())
                ) {
                    $(value).show();
                } else {
                    $(value).hide();
                }
            });
    });

    $(document).on("click", ".clear_breadcrumb_search", function () {
        $("input#search_breadcrumb").val("");
        $(this).parent().find("i.clear_breadcrumb_search").hide();
        $(this).parents().find("ul.updated li").show();
    });

    $(document).on("click", ".interactive", function () {
        var container = $(this).parents().eq(2);
        var list = container.find("ul");
        if (container.hasClass("active")) {
            container.removeClass("active");
        } else {
            $(".interactive_breadcrumb .container").removeClass("active");
            container.addClass("active");
        }
    });

    $(document).on("click", ".update_breadcrumb_title", function () {
        var el = $(this);
        $(this)
            .parent()
            .find(".update_breadcrumb_title")
            .removeClass("selected");
        $(this).addClass("selected");
        var interactive = $(this).parent().parent().find(".interactive");
        var new_id = $(this).attr("data-id");
        var new_link = $(this).attr("data-link");
        var new_text = $(this).text();
        interactive.attr("data-id", new_id);
        interactive.attr("data-link", new_link);
        interactive.find("span").text(new_text);
        interactive.find("span").text(new_text);
        $(this).parents().eq(3).removeClass("active");
        var initial_id = $(this).attr("data-initial-id");
        var type = $(this)
            .parents()
            .eq(1)
            .find(".interactive")
            .attr("data-type");
        var flag = "next";
        var formdata = { initial_id: initial_id, type: type, flag: flag };

        $.ajax({
            url: "ajax_show_breadcrumb_options.php",
            type: "post",
            data: formdata,
            success: function (response) {
                var last_item = el.parents().eq(3);
                last_item.nextAll().remove();
                if ($(".interactive_breadcrumb").children().length <= 3) {
                    if (!last_item.hasClass("breadcrumb_seperator")) {
                        last_item.addClass("breadcrumb_seperator");
                    }
                    $(".interactive_breadcrumb").append(response);
                }
            },
        });
    });
    $(document).on("click", ".gotolink", function () {
        window.location.href = $(this)
            .parent()
            .find(".interactive")
            .attr("data-link");
    });

