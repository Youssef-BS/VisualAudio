    /*wishlist ajax call*/
    $(document).on("click",'.update-wishlist', function(e){
		e.preventDefault();
		$(this).addClass('activated');
		$(this).addClass('clicked');
		var action;
		var origin;
        var formData;
        var activate_popup = false;
        var wishlistInnerScroll = $('#mini-wishlist-panel').scrollTop();
        var language = $('#language').val();
		if($(this).hasClass('listing'))
		{
			action = 'add_to_wishlist_now';
            var products_id = $('.update-wishlist.activated').attr('data-product-id');
            formData = {products_id:products_id, action:action, 'language':language};
            activate_popup = true;
		}
		else if($(this).hasClass('quick-view-add') || $(this).hasClass('final'))
		{
            var form;
            if($(this).hasClass('quick-view-add')){
                form =  $("#quickview_cart_quantity");    
            }
            else{
                form =  $("#cart_quantity_form");
            }

            form.append($('<input/>').attr('type', 'hidden').attr('name', 'action').attr('value', 'add_to_wishlist_now'));   
            form.append('<input type="hidden" name="wishlist">');
            form.append('<input type="hidden" name="product_page_wishlist">');
            formData = form.serialize();
            activate_popup = true;
		}
        else if($(this).hasClass('wishlist-delete'))
        {           
            //console.log("delete"); 
            if($("#wishlist_form").length > 0)
            {
                var actionForm = $("#wishlist_form")
                actionForm.append('<input type="hidden" name="wishlist_page">');    
            }
            else
            {
                var actionForm = $("#mini-wishlist-form")
                
            }
            actionForm.append($('<input/>').attr('type', 'hidden').attr('name', 'delete_prod').attr('value', 'delete_prod'));            
            action = 'remove';
            $('.add_wishprod[value="' + $(this).attr('data-product-id') + '"]').prop('checked', true);
            actionForm.append('<input type="hidden" name="language" value="' + language + '">');
            formData = actionForm.serialize();                
            activate_popup = true;
        }
        else if($(this).hasClass('update-wishlist-quantity'))
        {
			action = 'update_wishlist_quantity';
            var products_id = $('.update-wishlist.activated').attr('data-product-id');
            var new_quantity = parseInt($('.update-wishlist.activated').closest('.productWishlistCart').find('.wishlist_quantity_value').val());
            var flag;
            var multiplier = parseInt($(this).closest('.actionbar').find('#multiples_conversion_value').val());
            if($(this).hasClass('increase-wishlist-quantity'))
            {
                flag = 'up';
                if(multiplier > 1)
                {
                    new_quantity+=multiplier;
                }
                else
                {
                    new_quantity = parseInt(new_quantity) + 1;
                }
            }
            else if($(this).hasClass('decrease-wishlist-quantity'))
            {
                if(multiplier > 1)
                {
                    new_quantity -= multiplier;
                }
                else
                {
                    new_quantity = parseInt(new_quantity) - 1;
                }
                
                if(new_quantity <= 0)
                {
                    if(multiplier > 1)
                    {
                        new_quantity = multiplier;
                    }
                    else
                    {
                        new_quantity = 1;
                    }                    
                }
                flag = 'down';
            }
            
            formData = {products_id:products_id, action:action, new_quantity:new_quantity, flag:flag, wishlist_page:true};
            //console.log(formData);
            activate_popup = true;
        }
        else if($(this).hasClass('refresh'))
        {
            formData = {refresh:true};
        }
        else
        {
            formData = $('#wishlist_form').serialize();
        }
        //console.log(formData);
        if(formData !== undefined){
            $.ajax({
                url: 'ajax_update_wishlist.php',
                type: 'post',
                data: formData,
                success: function(response) {
                    //console.log("response");
                    //console.log(response);
                    var obj = JSON.parse(response);
                    //console.log(obj);
                    //console.log("here");
                    if(obj['redirect'] != "" && obj['redirect'] != null)
                    {
                        window.location = obj['redirect'];
                    }
                    else
                    {
                        if($('.wishlist-number').length > 0){
                            //console.log("if counter length");
                          $('.wishlist-number').html(obj['wishlist_counter']);
						  $('.wishlist-number').addClass('activated');
                        }
                        if($('#wishlist_form').length > 0){
                            replaceHtml(document.getElementById('wishlist_dynamic_content'), obj['wishlist_container']);
                        }
                        if($('#wishlist-items-container').length){
                            var replace_item = document.getElementById('wishlist-cart-items-wrapper');
                            replaceHtml(replace_item, obj['wishlist_mini']);
                            $('#mini-wishlist-panel').scrollTop(wishlistInnerScroll);
                        }
                        if(!$("#wishlist-items-container").hasClass('active') && $('#wishlist_form').length == 0 && activate_popup)	{
                            $('#shopping-cart-items-container').removeClass('active');
                            $("#wishlist-items-container").addClass('active');
                        }
                        if(parseInt(obj['wishlist_counter']) > 0)
                        {
                            $('.contact-form.from-wishlist').show();
                        }
                        else
                        {
                            $('.contact-form.from-wishlist').hide();
                        }
                    }
                },
                error: function(xhr, desc, err) {
                }
            });
        }
        if($('.update-wishlist').length > 0){
            $('.update-wishlist').removeClass('activated');
        }
		return false;
	});


    $(document).on("click", ".update-shopping-cart", function (e) {
        e.preventDefault();
        $(this).addClass("activated");
        var action;
        var origin;
        var checkShoppingCartEnabled = false;
        var allowAddToCart = false;
        var el;

        if ($(this).hasClass("listing")) {
            action = "add_product_from_listing";
            origin = "product-listing";
        } else if ($(this).hasClass("quick-view-add")) {
            action = "add_product";
            origin = "product-quickview";
            checkShoppingCartEnabled = true;
        } else if ($(this).hasClass("product-set-add")) {
            action = "add_product";
            origin = "product-set";
            $(".product_sets .cart_quantity_form").removeClass("active");
            $(this).closest(".cart_quantity_form").addClass("active");
            el = $(this);
        } else if ($(this).hasClass("add-to-cart-wishlist")) {
            action = "add_product";
            origin = "wishlist-form";
            el = $(this);
        } else if ($(this).hasClass("add-all-to-cart-wishlist")) {
            action = "update_product";
            origin = "wishlist-form-add-all";
        } else {
            action = "add_product";
            origin = "product-page";
            checkShoppingCartEnabled = true;
        }

        if (checkShoppingCartEnabled) {
            if (validateAddToCart()) {
                allowAddToCart = true;
            }
        } else {
            allowAddToCart = true;
        }
        if (allowAddToCart) {
            ajaxRefreshShoppingCart(action, origin, el);
        }
        return false;
    });

    /*cart automatic delete on click*/
    /*$(document).on("click", ".cart_close", function () {
        $(this)
            .parent()
            .find(".cart_delete_product_value")
            .prop("checked", true);
        var action = "update_product";
        var origin = "shopping-cart";
        ajaxRefreshShoppingCart(action, origin);
    });*/

    /*cart automatic delete on click*/
	$(document).on("click",'.shopping_cart_dynamic_content .cart_close', function(){
		$(this).parent().find(".cart_delete_product_value").prop( "checked", true );
		var action = 'update_product';
        var origin = 'shopping-cart';
		ajaxRefreshShoppingCart(action, origin);
	});

    $(document).on("click", ".minicart_close", function () {
        $(this)
            .parent()
            .find(".minicart_delete_product_value")
            .prop("checked", true);
        var action = "update_product";
        var origin = "mini-shopping-cart";
        ajaxRefreshShoppingCart(action, origin);
    });

    $(document).on(
        "blur",
        ".cart_quantity_form .cart_quantity_value",
        function () {
            if (isNaN($(this).val()) || parseInt($(this).val()) <= 0) {
                $(this).val("1");
            }

            var field = $(this).parent().find(".cart_quantity_value");
            var cart_quantity = parseInt(field.val());
            var multiplier = parseInt(
                $(this)
                    .parent()
                    .find("input[type=hidden][name=multiples_conversion]")
                    .val()
            );
            var solidQuantity = parseInt($("#solid_quantity").val());
            if (multiplier > 1) {
                if (cart_quantity % multiplier != 0) {
                    cart_quantity = solidQuantity;
                    $(".unit_special_quantity").css("font-weight", "bold");
                }
            }

            max_quantity = getProductMaxQuantity();

            if (cart_quantity > max_quantity) {
                cart_quantity = max_quantity;
                if (multiplier > 1) {
                    while (cart_quantity % multiplier !== 0) {
                        cart_quantity--;
                    }
                }
                $(".max-quantity-warning").show();
            } else {
                $(".max-quantity-warning").hide();
            }
            field.val(cart_quantity);
            $("#solid_quantity").val(cart_quantity);
            var action = "update_product";
            var origin = "shopping-cart";
            ajaxRefreshShoppingCart(action, origin);
        }
    );

    $(document).on("change", ".set_product_attribute", function () {
        var total_options = 0;
        var total_selected_options = 0;
        var field_container = $(this).closest(".attributes.aligner");
        var fields = field_container.find(
            '.set_product_attribute:not([data-option="' +
            $(this).attr("data-option") +
            '"])'
        );
        var active_option = $(this).attr("data-option");
        var id = {};
        var allow_reset;
        var selected_ids = {};

        if ($(this).val() !== "#") {
            var atribute_value = $(this).val();
            var res = atribute_value.split("_");
            id[res[0]] = [];
            id[res[0]].push(res[1]);
        } else {
            var reset_option = $(this).attr("data-option");
            id[reset_option] = [];
            id[reset_option].push("reset");
            allow_reset = reset_option;
        }
        field_container.find(".set_product_attribute").each(function () {
            total_options++;
            var selected_option_id = $(this).find("option:selected").val();
            if (selected_option_id !== "#") {
                total_selected_options++;
            }
            selected_ids[$(this).attr("data-option")] = selected_option_id;
        });

        fields.each(function () {
            if ($(this).find("option").length > 1) {
                $(this)
                    .find("option")
                    .each(function () {
                        if ($(this).val() !== "#") {
                            var atribute_value = $(this).val();
                            var res = atribute_value.split("_");
                            if (id[res[0]] == undefined) {
                                id[res[0]] = [];
                            }
                            id[res[0]].push(res[1]);
                        }
                    });
            } else {
                var option = $(this).attr("data-option");
                id[option] = [];
                id[option].push(null);
            }
        });
        if (total_options == total_selected_options) {
            $(this)
                .closest(".cart_quantity_form")
                .find(".product-set-add")
                .removeAttr("disabled");
        } else {
            $(this)
                .closest(".cart_quantity_form")
                .find(".product-set-add")
                .attr("disabled", "disabled");
        }
        var products_id = $(this)
            .closest("form")
            .find('input[type="hidden"][name="set_products_id"]')
            .val();

        var selected_string = $(this).val();
        var selected_option = selected_string.split("_");
        selected_option = selected_option[0];
        var formData = {
            products_id: products_id,
            id: id,
            selected_option: selected_option,
            selected_ids: selected_ids,
            action: "process",
        };
        $.ajax({
            url: "ajax_get_attributes_combinations.php",
            type: "post",
            data: formData,
            success: function (response) {
                var obj = JSON.parse(response);

                field_container;
                var data_fields = "";
                $.each(obj, function (index, value) {
                    if (active_option != index || allow_reset == index) {
                        $(
                            "#set_product_attribute_" +
                            products_id +
                            "_" +
                            index
                        ).replaceWith(value);
                    }
                });
            },
        });

        if (total_selected_options > 0) {
            $(
                '.clear-set-selections[data-product="' + products_id + '"]'
            ).removeClass("d-none");
        } else {
            $(
                '.clear-set-selections[data-product="' + products_id + '"]'
            ).addClass("d-none");
        }
    });
    /*clear all set attribute selections*/
    $(document).on("click", ".clear-set-selections", function () {
        var products_id = $(this).attr("data-product");
        $('.set_product_attribute[data-product="' + products_id + '"]').each(
            function () {
                $(this).val("#");
            }
        );
        $(
            '.set_product_attribute[data-product="' + products_id + '"]:first'
        ).trigger("change");
    });

    /*sticky add to cart product quantity*/
    var fieldStickyQuantity;
    $(".widget_quantity .la-plus").click(function () {
        $(
            "#cart_quantity_form .productQuantity .increase-product-quantity"
        ).click();
    });
    $(".widget_quantity .la-minus").click(function () {
        $(
            "#cart_quantity_form .productQuantity .decrease-product-quantity"
        ).click();
    });

    /*sticky add to cart button*/
    $(".sticky-add-to-cart").click(function () {
        $(".productinfo .social .mainimagebutton1").click();
    });

    $(document).on("click", ".increase-product-quantity", function () {
        var field = $(this).parent().find(".cart_quantity_value");
        var cart_quantity = parseInt(field.val());
        var multiplier = parseInt(
            $(
                ".cart_quantity_form input[type=hidden][name=multiples_conversion]"
            ).val()
        );
        if (multiplier > 1) {
            cart_quantity += multiplier;
        } else {
            cart_quantity++;
        }
        var max_quantity = getProductMaxQuantity();
        if (cart_quantity > max_quantity) {
            cart_quantity = max_quantity;
            if (multiplier > 1) {
                while (cart_quantity % multiplier !== 0) {
                    cart_quantity--;
                }
            }
            $(this).parent().find(".max-quantity-warning").show();
        } else {
            $(this).parent().find(".max-quantity-warning").hide();
        }
        field.val(cart_quantity);
        $("#solid_quantity").val(cart_quantity);
        /*get value for sticky add to cart*/
        fieldStickyQuantity = $(this)
            .parent()
            .find(".cart_quantity_value")
            .val();
        $(".widget_quantity input.cart_quantity_value").val(
            fieldStickyQuantity
        );
        var action = "update_product";
        var origin = "shopping-cart";
        ajaxRefreshShoppingCart(action, origin);
    });

    $(document).on("click", ".decrease-product-quantity", function () {
        $(this).parent().find(".max-quantity-warning").hide();
        var field = $(this).parent().find(".cart_quantity_value");
        var cart_quantity = parseInt(field.val());
        var multiplier = parseInt(
            $(
                ".cart_quantity_form input[type=hidden][name=multiples_conversion]"
            ).val()
        );
        if (multiplier > 1) {
            cart_quantity -= multiplier;
        } else {
            cart_quantity--;
        }
        if (cart_quantity <= 0) {
            if (multiplier > 1) {
                cart_quantity = multiplier;
            } else {
                cart_quantity = 1;
            }
        }
        field.val(cart_quantity);
        $("#solid_quantity").val(cart_quantity);
        /*get value for sticky add to cart*/
        fieldStickyQuantity = $(this)
            .parent()
            .find(".cart_quantity_value")
            .val();
        $(".widget_quantity input.cart_quantity_value").val(
            fieldStickyQuantity
        );
        var action = "update_product";
        var origin = "shopping-cart";
        ajaxRefreshShoppingCart(action, origin);
    });

    $(document).on(
        "blur",
        ".shopping_cart_dynamic_content .cart_quantity_action",
        function () {
            var field = $(this);
            if (field.val() != "") {
                var cart_quantity = parseInt(field.val());
                $("#solid_quantity").val(cart_quantity);
                var action = "update_product";
                var origin = "shopping-cart";
                ajaxRefreshShoppingCart(action, origin);
            }
        }
    );
    $(document).on("click", ".increase-cart-quantity", function () {
        var multiplier = parseInt(
            $(this).parent().find("#multiples_conversion_value").val()
        );
        var field = $(this)
            .parent()
            .find('input[type="text"][name="cart_quantity[]"]');
        var cart_quantity = parseInt(field.val());
        if (multiplier > 1) {
            cart_quantity += multiplier;
        } else {
            cart_quantity++;
        }
        field.val(cart_quantity);
        $("#solid_quantity").val(cart_quantity);
        var action = "update_product";
        var origin = "shopping-cart";
        window.localStorage.setItem(
            "elementOffsetY",
            $("#mini-cart-panel").scrollTop()
        );
        ajaxRefreshShoppingCart(action, origin, $(this));
    });
    $(document).on("click", ".decrease-cart-quantity", function () {
        var multiplier = parseInt(
            $(this).parent().find("#multiples_conversion_value").val()
        );
        var field = $(this)
            .parent()
            .find('input[type="text"][name="cart_quantity[]"]');
        var cart_quantity = parseInt(field.val());
        if (multiplier > 1) {
            cart_quantity -= multiplier;
        } else {
            cart_quantity--;
        }

        if (cart_quantity <= 0) {
            if (multiplier > 1) {
                cart_quantity = multiplier;
            } else {
                cart_quantity = 1;
            }
        }
        field.val(cart_quantity);
        $("#solid_quantity").val(cart_quantity);
        var action = "update_product";
        var origin = "shopping-cart";
        window.localStorage.setItem(
            "elementOffsetY",
            $("#mini-cart-panel").scrollTop()
        );
        ajaxRefreshShoppingCart(action, origin);
    });

    $(document).on("click", ".increase-minicart-quantity", function () {
        var field = $(this)
            .parents()
            .eq(1)
            .find('input[type="text"][name="minicart_quantity[]"]');
        var cart_quantity = parseInt(field.val());
        var multiplier = parseInt(
            $(this)
                .parents()
                .eq(2)
                .find("input[type=hidden][name=multiples_conversion]")
                .val()
        );
        if (multiplier > 1) {
            cart_quantity += multiplier;
        } else {
            cart_quantity++;
        }
        field.val(cart_quantity);
        $("#solid_quantity").val(cart_quantity);
        var action = "update_product";
        var origin = "mini-shopping-cart";
        window.localStorage.setItem(
            "elementOffsetY",
            $("#mini-cart-panel").scrollTop()
        );
        ajaxRefreshShoppingCart(action, origin);
    });
    $(document).on("click", ".decrease-minicart-quantity", function () {
        var field = $(this)
            .parents()
            .eq(1)
            .find('input[type="text"][name="minicart_quantity[]"]');
        var cart_quantity = parseInt(field.val());
        var multiplier = parseInt(
            $(this)
                .parents()
                .eq(2)
                .find("input[type=hidden][name=multiples_conversion]")
                .val()
        );
        if (multiplier > 1) {
            cart_quantity -= multiplier;
        } else {
            cart_quantity--;
        }

        if (cart_quantity <= 0) {
            if (multiplier > 1) {
                cart_quantity = multiplier;
            } else {
                cart_quantity = 1;
            }
        }
        field.val(cart_quantity);
        $("#solid_quantity").val(cart_quantity);
        var action = "update_product";
        var origin = "mini-shopping-cart";
        window.localStorage.setItem(
            "elementOffsetY",
            $("#mini-cart-panel").scrollTop()
        );
        ajaxRefreshShoppingCart(action, origin);
    });

